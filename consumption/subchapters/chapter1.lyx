\begin_layout Standard
\begin_inset Formula 
\begin{align*}
a_{consu}(age) & =\begin{cases}
0.2 & age\le5\\
0.3 & 5<age\le10\\
0.4 & 10<age<15\\
1 & 15<age\le45\\
0.7 & 45<age\le65\\
0.6 & age>65
\end{cases}
\end{align*}

