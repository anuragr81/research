\begin_layout Standard
\begin_inset Formula 
\begin{alignat}{1}
u(\nu_{t},r_{t}) & =\alpha log(\nu_{t}-\bar{\nu})+(1-\alpha)log(r_{t}-\bar{r})\label{eq:basic_utility}
\end{alignat}

