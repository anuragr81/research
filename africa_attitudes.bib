@article{KarmakarSWU1978,
title = "Subjectively weighted utility: A descriptive extension of the expected utility model",
journal = "Organizational Behavior and Human Performance",
volume = "21",
number = "1",
pages = "61 - 72",
year = "1978",
issn = "0030-5073",
doi = "https://doi.org/10.1016/0030-5073(78)90039-9",
url = "http://www.sciencedirect.com/science/article/pii/0030507378900399",
author = "Uday S. Karmarkar",
abstract = "A parsimonious extension to the Expected Utility (EU) model is proposed involving one additional parameter. The model is an example of a Subjectively Weighted Utility (SWU) model, which differs from the EU model only in the way probabilities are incorporated. The probability transformation is of a form that has appeared in the literature on human information processing and the additional parameter  has been interpreted as an index of information processing performance. The SWU model is shown to have considerable descriptive ability. It is discussed in the context of the substitutability axiom, distinct utility curves, insurance purchasing and gambling behavior, and probability variance preferences."
